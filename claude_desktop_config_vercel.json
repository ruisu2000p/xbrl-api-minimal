{
  "mcpServers": {
    "xbrl-financial": {
      "command": "npx",
      "args": ["--loglevel=error", "shared-supabase-mcp-minimal@1.8.1"]
    },
    "vercel-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@vercel/mcp"
      ],
      "env": {
        "VERCEL_ACCESS_TOKEN": "YOUR_VERCEL_ACCESS_TOKEN_HERE"
      }
    }
  }
}